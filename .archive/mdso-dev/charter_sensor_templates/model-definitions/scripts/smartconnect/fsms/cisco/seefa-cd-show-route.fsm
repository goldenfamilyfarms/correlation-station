Value TIMESTAMP (\w+\s\w+\s+\d+\s\d+:\d+:\d+.\d+\s\w+)
Value NETWORK (.*)
Value TYPE (.*)
Value DISTANCE (\d+)
Value METRIC (\d+)
Value NEXT_HOP ((?:[a-f0-9]{1,4}):(?:(?:[a-f0-9]{1,4}:)|:){1,7}(?:[a-f0-9]{1,4})|::(?:[a-f0-9]{1,4})|\d+.\d+.\d+.\d+)
Value INTERFACE (.*)


Start
  ^${TIMESTAMP}
  ^Routing\sentry\sfor\s${NETWORK}$$
  ^\s+Known\svia\s"${TYPE}",\sdistance\s${DISTANCE},\smetric\s${METRIC}
  ^\s+${NEXT_HOP},\sfrom\s\d+.\d+.\d+.\d+,\svia\s${INTERFACE}
  ^\s+${NEXT_HOP}
  ^\s+\d+.\d+.\d+.\d+,\sfrom\s.\d+.\d+.\d+.\d+,\svia\s${INTERFACE}
  ^\s+directly\sconnected,\svia\s${INTERFACE}$$
  ^\s+Route\smetric\sis\s\d+$$ -> Record
  ^%\sNetwork\snot\sin\stable -> Record

EOF
